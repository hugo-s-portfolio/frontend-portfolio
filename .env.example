# URL del sitio
SITE_URL="https://example.com"

# API del backend NestJS
NEXT_PUBLIC_BACK_API="https://api.example.com/api/v1"

# Credenciales de autenticación (server-side only, sin prefijo NEXT_PUBLIC_)
STRAPI_IDENTIFIER="email@example.com"
STRAPI_PASSWORD="your-password-here"

# Clave de encriptación para Redux persist (client-side, necesita NEXT_PUBLIC_)
NEXT_PUBLIC_SECRET_PORTFOLIO="generate-a-random-32-char-string"

# Timeout de sesión en milisegundos (4 horas)
NEXT_PUBLIC_STRAPI_TIMEOUT="14400000"
